<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:liftOnScroll="true"
        android:fitsSystemWindows="true">
        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="?attr/collapsingToolbarLayoutMediumSize"
            style="?attr/collapsingToolbarLayoutMediumStyle">
            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/up_actions_tb"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:minHeight="?attr/actionBarSize"
                app:title="@string/actions"
                app:navigationIcon="@drawable/back"
                app:layout_scrollFlags="scroll|enterAlways|snap"
                app:layout_scrollEffect="compress"
                android:elevation="0dp" />
        </com.google.android.material.appbar.CollapsingToolbarLayout>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="56dp"
                android:clickable="true"
                android:focusable="true">
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i1_ssw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i1_s"
                    android:orientation="vertical"
                    android:id="@+id/up_actions_i1_hc"
                    android:paddingTop="12dp"
                    android:background="?attr/selectableItemBackground">
                    <TextView
                        android:id="@+id/up_actions_i1_h"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:textColor="?attr/colorOnSurface"
                        android:textSize="20sp"
                        android:text="@string/alarm" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:paddingBottom="12dp"
                        android:textColor="?attr/colorOnSurfaceVariant"
                        android:textSize="16sp"
                        android:text="@string/alarm_d"
                        android:id="@+id/up_actions_i1_c" />
                </LinearLayout>

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:rotation="90"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i1_sw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i1_s"
                    app:layout_constraintLeft_toRightOf="@id/up_actions_i1_hc"
                    android:id="@+id/up_actions_i1_ssw" />

                <com.google.android.material.materialswitch.MaterialSwitch
                    android:id="@+id/up_actions_i1_sw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:saveEnabled="false"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i1_s"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    android:layout_marginEnd="10dp" />

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/up_actions_i1_hc"
                    android:id="@+id/up_actions_i1_s" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="56dp"
                android:clickable="true"
                android:focusable="true">
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i2_ssw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i2_s"
                    android:orientation="vertical"
                    android:id="@+id/up_actions_i2_hc"
                    android:paddingTop="12dp"
                    android:background="?attr/selectableItemBackground">
                    <TextView
                        android:id="@+id/up_actions_i2_h"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:textColor="?attr/colorOnSurface"
                        android:textSize="20sp"
                        android:text="@string/intruderphoto" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:paddingBottom="12dp"
                        android:textColor="?attr/colorOnSurfaceVariant"
                        android:textSize="16sp"
                        android:text="@string/intruderphoto_d"
                        android:id="@+id/up_actions_i2_c" />
                </LinearLayout>

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:rotation="90"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i2_sw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i2_s"
                    app:layout_constraintLeft_toRightOf="@id/up_actions_i2_hc"
                    android:id="@+id/up_actions_i2_ssw" />

                <com.google.android.material.materialswitch.MaterialSwitch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i2_s"
                    app:layout_constraintRight_toRightOf="parent"
                    android:layout_marginEnd="10dp"
                    android:id="@+id/up_actions_i2_sw"
                    android:saveEnabled="false"
                    android:enabled="false" />

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/up_actions_i2_hc"
                    android:id="@+id/up_actions_i2_s" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="56dp"
                android:clickable="true"
                android:focusable="true"
                android:background="?attr/selectableItemBackground"
                android:id="@+id/up_actions_notification">
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i3_sw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i3_s"
                    android:orientation="vertical"
                    android:id="@+id/up_actions_i3_hc"
                    android:paddingTop="12dp">
                    <TextView
                        android:id="@+id/up_actions_i3_h"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:textColor="?attr/colorOnSurface"
                        android:textSize="20sp"
                        android:text="@string/notification" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:paddingBottom="12dp"
                        android:textColor="?attr/colorOnSurfaceVariant"
                        android:textSize="16sp"
                        android:text="@string/notification_d"
                        android:id="@+id/up_actions_i3_c" />
                </LinearLayout>

                <com.google.android.material.materialswitch.MaterialSwitch
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i3_s"
                    app:layout_constraintRight_toRightOf="parent"
                    android:layout_marginEnd="10dp"
                    android:id="@+id/up_actions_i3_sw"
                    android:saveEnabled="false"
                    android:enabled="false" />

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/up_actions_i3_hc"
                    android:id="@+id/up_actions_i3_s" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="56dp"
                android:clickable="true"
                android:focusable="true">
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i4_ssw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i4_s"
                    android:orientation="vertical"
                    android:id="@+id/up_actions_i4_hc"
                    android:paddingTop="12dp"
                    android:background="?attr/selectableItemBackground">
                    <TextView
                        android:id="@+id/up_actions_i4_h"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:textColor="?attr/colorOnSurface"
                        android:textSize="20sp"
                        android:text="@string/sms" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:paddingBottom="12dp"
                        android:textColor="?attr/colorOnSurfaceVariant"
                        android:textSize="16sp"
                        android:text="@string/sms_d"
                        android:id="@+id/up_actions_i4_c" />
                </LinearLayout>

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:rotation="90"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i4_sw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i4_s"
                    app:layout_constraintLeft_toRightOf="@id/up_actions_i4_hc"
                    android:id="@+id/up_actions_i4_ssw" />

                <com.google.android.material.materialswitch.MaterialSwitch
                    android:id="@+id/up_actions_i4_sw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:saveEnabled="false"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i4_s"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    android:layout_marginEnd="10dp"
                    android:enabled="false" />

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/up_actions_i4_hc"
                    android:id="@+id/up_actions_i4_s" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="56dp"
                android:clickable="true"
                android:focusable="true">
                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i5_ssw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i5_s"
                    android:orientation="vertical"
                    android:id="@+id/up_actions_i5_hc"
                    android:paddingTop="12dp"
                    android:background="?attr/selectableItemBackground">
                    <TextView
                        android:id="@+id/up_actions_i5_h"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:textColor="?attr/colorOnSurface"
                        android:textSize="20sp"
                        android:text="@string/voice" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="16dp"
                        android:paddingRight="16dp"
                        android:paddingBottom="12dp"
                        android:textColor="?attr/colorOnSurfaceVariant"
                        android:textSize="16sp"
                        android:text="@string/voice_d"
                        android:id="@+id/up_actions_i5_c" />
                </LinearLayout>

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:rotation="90"
                    app:layout_constraintRight_toLeftOf="@id/up_actions_i5_sw"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i5_s"
                    app:layout_constraintLeft_toRightOf="@id/up_actions_i5_hc"
                    android:id="@+id/up_actions_i5_ssw" />

                <com.google.android.material.materialswitch.MaterialSwitch
                    android:id="@+id/up_actions_i5_sw"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:saveEnabled="false"
                    app:layout_constraintBottom_toTopOf="@id/up_actions_i5_s"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    android:layout_marginEnd="10dp"
                    android:enabled="false" />

                <com.google.android.material.divider.MaterialDivider
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/up_actions_i5_hc"
                    android:id="@+id/up_actions_i5_s" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</androidx.coordinatorlayout.widget.CoordinatorLayout>